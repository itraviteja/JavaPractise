package main;

import java.util.Scanner;

public class Runner 
{
	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		System.out.println("enter the long value");
	
	       String num = 	s.next();

	        int prod = 0;

	        for (int i=0; i < num.length()-4; i++)
	        {
	            int tmp = (num.charAt(i) - '0')*
	                      (num.charAt(i+1) - '0')*
	                      (num.charAt(i+2) - '0')*
	                      (num.charAt(i+3) - '0')*
	                      (num.charAt(i+4) - '0');
	            if (tmp > prod){
	                prod = tmp;
	            }
	        }

	        return  prod;
		
	
	}

}

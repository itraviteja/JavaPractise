package main;

import java.util.Scanner;

public class Runner 
{
	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		System.out.println("enter the long value");
		Long number=s.nextLong();

		int temp=0;
		int pro=0;
		while(number!=0)
		{
			for(int i=0;i<3;i++)
			{
				temp*=number/10;
			}
			if(temp>pro)
				pro=temp;
			number=number%10;
			
		}
		System.out.println(pro);
		
	
	}

}
